{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {

        "template": "./node_modules/ink-docstrap/template",
        "encoding": "utf8",
        "destination": "./jsdocs/",
        "recurse": true
    }
}